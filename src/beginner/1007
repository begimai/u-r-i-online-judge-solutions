import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int A;
		int B;
		int C;
		int D;
		int DIFERENCA ;
		A = scan.nextInt();
		B = scan.nextInt();
		C = scan.nextInt();
		D = scan.nextInt();
		DIFERENCA  = A * B - C * D;
		System.out.printf("DIFERENCA = %d\n", DIFERENCA);
	}
}
