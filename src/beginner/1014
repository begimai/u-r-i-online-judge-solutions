import java.util.Locale;
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner scan = new Scanner(System.in);
		float X;
		float Y;
		X = scan.nextFloat();
		Y = scan.nextFloat();
		float result  = X / Y;
		System.out.printf("%.3f km/l\n", result);

	}
}
