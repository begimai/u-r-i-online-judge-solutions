import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int time ;
		int speed ;
		speed = scan.nextInt();
		time = scan.nextInt();
		int distance   = speed*time;
		double liters = distance/12.0;
		System.out.printf("%.3f\n", liters );

	}
}
