import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int X;
		X = scan.nextInt();
		int years = 0;
		int months  = 0;
		int days = 0;
		if (X != 0)
		{
			years  =+ X /365;
			months  =+ X % 365 / 30;
			days  =+X%365%30 /1 ;
		}
		
		System.out.printf("%d ano(s)\n", years);
		System.out.printf("%d mes(es)\n", months);
		System.out.printf("%d dia(s)\n", days);
		
	}
}
